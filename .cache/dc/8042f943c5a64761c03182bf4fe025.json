scrollbar-width: none; /* hide scrollbar for Firefox */\r\n\r\n  position: relative;\r\n  padding: 0;\r\n  margin: 0;\r\n}\r\n\r\n*::-webkit-scrollbar {\r\n  /* hide scrollbar for Chrome, Safari and Opera */\r\n  display: none;\r\n}\r\n\r\n.noise {\r\n  display: block !important;\r\n  position: fixed;\r\n  left: 0%;\r\n  top: 0%;\r\n  right: 0%;\r\n  bottom: 0%;\r\n  z-index: 99999;\r\n  background-image: url(https://raw.githubusercontent.com/haroldao/medias/main/Noise_Animated_Texture.gif);\r\n  background-position: 0px 0px;\r\n  background-size: auto;\r\n  opacity: 0.06;\r\n  pointer-events: none;\r\n}\r\n\r\n.header_container {\r\n  position: fixed;\r\n  z-index: 1000;\r\n  top: 0;\r\n  height: auto;\r\n}\r\n\r\n.promo-bar_container {\r\n  height: var(--promo-bar_height);\r\n  padding: var(--padding);\r\n  background-color: var(--Primary);\r\n}\r\n\r\n.promo-bar_wrapper {\r\n  position: relative;\r\n  height: 100%;\r\n}\r\n\r\n.promo-bar_wrapper p {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  -ms-transform: translate(-50%, -50%);\r\n  transform: translate(-50%, -50%);\r\n  white-space: nowrap;\r\n}\r\n\r\n.navigation_container {\r\n  height: var(--navigation_height);\r\n  width: 100vw;\r\n  box-sizing: border-box;\r\n  border-top: 1px solid black;\r\n  border-bottom: 1px solid black;\r\n  padding: var(--padding);\r\n  background-color: whitesmoke;\r\n}\r\n\r\n.navigation_wrapper {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  height: 100%;\r\n}\r\n\r\n.navigation_logo {\r\n  position: relative;\r\n}\r\n\r\n.navigation_logo h2 {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  -ms-transform: translate(-50%, -50%);\r\n  transform: translate(-50%, -50%);\r\n  white-space: nowrap;\r\n}\r\n\r\n.hero_container {\r\n  margin-top: calc(var(--navigation_height) + var(--promo-bar_height));\r\n  width: 100%;\r\n  height: auto;\r\n  display: flex;\r\n  flex-direction: column;\r\n  border-bottom: 1px solid black;\r\n}\r\n\r\n.hero_image {\r\n  width: 100vw;\r\n  height: 37vw;\r\n}\r\n\r\n.flickity-container {\r\n  height: 100%;\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.carousel-cell {\r\n  height: 100%;\r\n  width: 100%;\r\n  overflow: hidden;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.carousel-cell img {\r\n  display: block;\r\n  object-fit: cover;\r\n  width: 100%; /* Set the width of the parent element */\r\n  height: 100%; /* Set the height of the parent element */\r\n  box-sizing: border-box;\r\n}\r\n\r\n.featured-text_container {\r\n  width: 100%;\r\n  height: 100px;\r\n  position: relative;\r\n  border-bottom: 1px solid black;\r\n}\r\n\r\n.section-text_container {\r\n  width: 100%;\r\n  height: 200px;\r\n  position: relative;\r\n  border-bottom: 1px solid black;\r\n}\r\n\r\n.product_container {\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n  border-bottom: 1px solid black;\r\n}\r\n\r\n.product-text_wrapper,\r\n.product-image_wrapper {\r\n  box-sizing: border-box;\r\n  height: 70vh;\r\n}\r\n\r\n.product-text_wrapper {\r\n  flex: 300px;\r\n}\r\n\r\n.product-image_wrapper {\r\n  flex: 10 1 auto;\r\n  overflow: hidden;\r\n}\r\n\r\n.product-image_wrapper img {\r\n  object-fit: cover;\r\n  position: absolute;\r\n  width: 100%;\r\n  height: 100%;\r\n  transition: all ease-in-out 0.5s;\r\n}\r\n\r\n.product-image_wrapper img:hover {\r\n  transform: scale(1.02);\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .product_container {\r\n    flex-direction: column;\r\n  }\r\n  .product-text_wrapper {\r\n    order: 2;\r\n    width: 100%;\r\n  }\r\n  .product-image_wrapper {\r\n    order: 1;\r\n    width: 100%;\r\n  }\r\n}\r\n\r\n/*font-family: 'Open Sans', sans-serif;*/\r\n#selectedValue1,\r\n#selectedValue2,\r\n#selectedValue3 {\r\n  border: none;\r\n  background-color: white;\r\n  color: black;\r\n}\r\n\r\n#dropdown1,\r\n#dropdown2,\r\n#dropdown3 {\r\n  border: none;\r\n  border-bottom: 1px solid black;\r\n}\r\n"},"lineCount":208}},"error":null,"hash":"5ee55f02